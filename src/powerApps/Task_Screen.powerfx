                          =/* gal_TaskGallery.Items  ―  변수 스코프 수정 & 위임 최적화 */
                          With(
                              /* 0) 날짜 상수: 서버 비교용 */
                              {
                                  dtToday   : Today(),
                                  dtYearAgo : DateAdd( Today(), -365,TimeUnit.Days )
