                                                      업무_종료일자 >= dtToday
                                                  ),
                                              /* default = "All" */
                                                  Filter(
                                                      Task_DB,
                                                      작성자           = name_label_1.Text,
                                                      업무_시작일자 >= dtYearAgo
                                                  )
                                          )
                                  },

                                  With(
                                      /* 2) 프로젝트 다중 선택 필터 (로컬) */
                                      {
                                          projRows :
                                              If(
                                                  IsEmpty( colSelectedProjects ),
                                                  baseRows,
                                                  Filter(
                                                      baseRows,
                                                      프로젝트명 in colSelectedProjects.Value
                                                  )
                                              )
